<h1>aficionado</h1>

  <div class="container">
    <div class="row">
      <div class="container col-sm-12 users">
        {{#each users}}
          <h2>{{this.id}}. {{this.name}}
        {{/each}}
      </div>
    </div>
  </div>
      <br><br>

<form method="POST" action="/" class="button-size">
  <input type="text" name="name" placeholder="Username"></input>
  <button type="submit" class="btn btn-secondary submit">Submit</button>
</form>

<script type="text/javascript" src="/javascript/script.js"></script>